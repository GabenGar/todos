{
  "name": "frontend",
  "version": "0.33.0",
  "description": "NextJS frontend for todo list.",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-codegen": "cd ./src/codegen/generators && npm run build",
    "start": "next start",
    "lint": "next lint && prettier --log-level=warn ./src --check",
    "format": "prettier --log-level=warn ./src --write",
    "dev": "next dev",
    "codegen": "npm run build-codegen && cd ../codegen && npm start -- ../frontend/src/codegen/generators ../frontend/src/codegen/output && cd ../frontend && prettier --log-level=warn ./src/codegen/output --write"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.2",
    "@hyperjump/json-schema": "^1.6.5",
    "ajv": "^8.12.0",
    "bcp-47": "^2.1.0",
    "clsx": "^2.0.0",
    "html5-qrcode": "^2.3.8",
    "iso-639-1": "^3.1.0",
    "nanoid": "^5.0.3",
    "negotiator": "^0.6.3",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1",
    "stringify-object": "^5.0.0"
  },
  "devDependencies": {
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/stringify-object": "^4.0.5",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "3.1.0",
    "sass": "^1.69.5",
    "typescript": "^5.2.2"
  }
}

{
  "compilerOptions": {
    "outDir": "./",
    "rootDir": "./",
    "target": "es2020",
    "lib": [
      "ES2020"
    ],
    "allowJs": false,
    "module": "Node16",
    "moduleResolution": "Node16",
    "incremental": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
  ],
  "exclude": []
}

{
  "name": "codegen",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "lint": "biome lint ./src && tsc --noEmit",
    "format": "biome check ./src --apply && tsc --noEmit"
  },
  "imports": {
    "#codegen": "./dist/src/codegen/index.js",
    "#lib/*": "./dist/src/lib/*/index.js"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.1",
    "@types/node": "^16.0.0",
    "typescript": "^5.2.2"
  }
}

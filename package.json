{
  "name": "todos",
  "version": "0.2.0",
  "description": "The list of todos in NextJS.",
  "keywords": [
    "NextJS"
  ],
  "engines": {
    "node": ">=18"
  },
  "private": true,
  "author": "GabenGar",
  "license": "MIT",
  "homepage": "https://github.com/GabenGar/todos#readme",
  "bugs": {
    "url": "https://github.com/GabenGar/todos/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GabenGar/todos.git"
  },
  "workspaces": [
    "frontend",
    "codegen"
  ],
  "scripts": {
    "build": "turbo build",
    "start": "cd frontend && npm start",
    "lint": "turbo lint",
    "format": "turbo format",
    "dev": "turbo dev",
    "prep": "npm run install-all && npm run build && npm run format",
    "codegen": "cd frontend && npm run codegen"
  },
  "devDependencies": {
    "turbo": "1.11.3"
  },
  "packageManager": "npm@8.7.0"
}

{
  "name": "frontend",
  "version": "0.39.0",
  "description": "NextJS frontend for todo list.",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-codegen": "cd ./codegen/generators && npm run build",
    "start": "serve ./dist -p 9001 --no-clipboard --no-port-switching",
    "lint": "next lint && prettier --log-level=warn ./src --check",
    "format": "prettier --log-level=warn ./src --write",
    "dev": "next dev --port 8001",
    "codegen": "npm run build-codegen && node ./scripts/codegen && prettier --log-level=warn ./codegen/output --write"
  },
  "dependencies": {
    "@hyperjump/json-schema": "^1.9.9",
    "@next/env": "15.2.4",
    "@repo/nodejs": "*",
    "@repo/ui": "*",
    "ajv": "^8.17.1",
    "bcp-47": "^2.1.0",
    "clsx": "^2.1.1",
    "html5-qrcode": "^2.3.8",
    "iso-639-1": "^3.1.5",
    "nanoid": "^5.1.5",
    "next": "15.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "server-only": "^0.0.1",
    "stringify-object": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.3.12",
    "@types/stringify-object": "^4.0.5",
    "eslint": "9.24.0",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "prettier": "3.5.3",
    "sass": "^1.86.3",
    "serve": "^14.2.4",
    "typescript": "^5.8.3"
  }
}

{
	"name": "json-schema-to-typescript",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"imports": {
		"#tests": "./dist/src/tests/index.js",
		"#lib": "./dist/src/transform/index.js"
	},
	"scripts": {
		"build": "tsc",
		"test": "npm run test-build && npm run test-format && node dist/scripts/test.js",
		"test-build": "cd tests && npm run build",
		"test-format": "biome check ./tests --apply",
		"lint": "biome lint ./ && tsc --noEmit",
		"format": "biome check ./ --apply"
	},
	"devDependencies": {
		"@biomejs/biome": "1.5.2",
		"@types/node": "^20.0.0",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"@hyperjump/json-schema": "^1.7.2"
	}
}
